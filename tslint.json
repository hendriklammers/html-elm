{
  "extends": [
    "tslint:latest",
    "tslint-config-prettier"
  ],
  "linterOptions": {
    "exclude": [
      "node_modules/**/*.ts"
    ]
  },
  "rules": {
    "interface-name" : [true, "never-prefix"],
    "member-access": false,
    "no-console": false,
    "object-literal-sort-keys": false,
    "ordered-imports": false,
    "quotemark": [true, "single", "jsx-double"],
    "no-var-requires": false,
    "interface-over-type-literal": false
  }
}
