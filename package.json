{
  "name": "html-elm",
  "version": "1.0.0",
  "description": "Convert html to elm-html",
  "repository": "github:hendriklammers/html-elm",
  "main": "./lib/index.js",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "start": "nodemon --watch src/**/*.ts --exec ts-node src/index.ts",
    "lint": "tslint -p tsconfig.json",
    "test": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "keywords": [
    "elm",
    "html",
    "parser",
    "convert"
  ],
  "author": "Hendrik Lammers <lammers@gmail.com> (http://www.hendriklammers.com)",
  "license": "MIT",
  "devDependencies": {
    "@types/htmlparser2": "^3.7.31",
    "@types/jest": "^24.0.0",
    "@types/node": "^10.12.24",
    "htmlparser2": "^3.10.0",
    "jest": "^24.1.0",
    "nodemon": "^1.18.10",
    "prettier": "^1.16.4",
    "ts-jest": "^23.10.5",
    "ts-node": "^8.0.2",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.3.3"
  },
  "bin": {
    "html-elm": "./lib/cli.js"
  }
}
